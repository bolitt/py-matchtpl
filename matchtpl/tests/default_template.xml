<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- <m:match version="1.0" xmlns:m="Tian's html Matching Template"> -->

<root>
	<!-- match list -->
	<array select="#secondary">list</array>

	<!-- match dict -->
	<map select="#person">
		<s key="job" select=".job"></s>
		<map key="address" select=".address"></map>
		<array key="name" select=".name">
			<s select=".firstname"></s>
			<map select=".nicknames"></map>
		</array>
	</map>
	
	<!-- match string -->
	<s>s</s>
</root>